{
  "functions": {
    "api/option-price.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/earnings.js": {
      "memory": 256,
      "maxDuration": 10
    },
    "api/scan-options.js": {
      "memory": 512,
      "maxDuration": 15
    },
    "api/strategy-recommend.js": {
      "memory": 512,
      "maxDuration": 15
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
